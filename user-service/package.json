{
  "name": "artoy_vault",
  "version": "1.0.0",
  "description": "ArtToyVault is an e-commerce platform designed to sell collectible art toys.",
  "author": "Cao Chien <caochientp1@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "preinstall": "chmod +x ./npmrc.sh && sh npmrc.sh",
    "proto:install": "yarn add @cqchien/artoy-vault-service-protos@latest",
    "proto:user": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./node_modules/@cqchien/artoy-vault-service-protos/dist/src/proto/user-service --ts_proto_out=src/modules/user ./node_modules/@cqchien/artoy-vault-service-protos/dist/src/proto/user-service/user.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "build:prod": "tsc -p tsconfig.build.json",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn run typeorm migration:generate -d ormconfig.ts",
    "migration:create": "yarn run typeorm migration:create",
    "migration:revert": "yarn run typeorm migration:revert -d ormconfig.ts",
    "migration:run": "yarn run typeorm migration:run -d ormconfig.ts",
    "schema:drop": "yarn run typeorm schema:drop",
    "watch:dev": "nodemon --config nodemon.json",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "prepare": "cd .. && husky install user-service/.husky"
  },
  "dependencies": {
    "@cqchien/artoy-vault-service-protos": "^1.0.3",
    "@grpc/grpc-js": "^1.11.1",
    "@grpc/proto-loader": "^0.7.13",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/throttler": "^6.1.1",
    "@nestjs/typeorm": "^10.0.1",
    "bcrypt": "^5.1.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nestjs-cls": "^4.4.1",
    "nodemon": "^3.1.4",
    "parse-duration": "^1.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "ts-proto": "^2.0.2",
    "tslib": "^2.6.2",
    "typeorm": "^0.3.19",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@handfish/hygen": "^6.1.6",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/testing": "^10.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.5.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-canonical": "^4.18.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-no-secrets": "^1.0.2",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^1.0.4",
    "eslint-plugin-unicorn": "^55.0.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "~15.2.9",
    "prettier": "^3.1.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
